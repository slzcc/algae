{
    "template": {
        "docs": "http://192.168.6.70/opsdev/devops-pipeline-template",
        "type": "default"
    },
    "department": {
        "name": "devops",
        "leader": ""
    },
    "deploy": {
        "service": {
            "name": "algae",
            "version": "1.0.0",
            "language": {
                "name": "golang",
                "type": "binary"
            }
        },
        "kubernetes": {
            "profile": {
                "kind": "deployment",
                "namespace": "devops",
                "replicas": 1
            },
            "cluster": [{
                "name": "aws_o",
                "api": "https://kubernetes-apiserver-ops-27e0f6d2ba6186cd.elb.ap-southeast-1.amazonaws.com:6443"
            }],
            "helm": {
                "url": "https://harbor.aws.ops.zhangyue-ops.com/chartrepo/haiwai",
                "repo": "harbor-aws-haiwai",
                "chart" : {
                    "name": "base-go",
                    "version": "1.0.0"
                }
            }
        },
        "jenkins": {
            "stage": {
                "helm": {"options": "--set volumeMounts[0].name=usr-local-timezone --set volumeMounts[0].mountPath=/usr/share/zoneinfo --set volumeMounts[0].readOnly=true --set volumes[0].name=usr-local-timezone --set volumes[0].hostPath.path=/usr/share/zoneinfo --set volumes[0].hostPath.type=DirectoryOrCreate --set affinitys.enabled=true --set readinessProbeHooks.create=true --set readinessProbeHooks.hooks.exec.command[0]='/bin/bash' --set readinessProbeHooks.hooks.exec.command[1]='-c' --set readinessProbeHooks.hooks.exec.command[2]='nc -z -v -n 127.0.0.1 80' --set readinessProbeHooks.hooks.initialDelaySeconds=5 --set readinessProbeHooks.hooks.periodSeconds=10 --set readinessProbeHooks.hooks.timeoutSeconds=5 --set readinessProbeHooks.hooks.successThreshold=1 --set readinessProbeHooks.hooks.failureThreshold=10 --set livenessProbeHooks.create=true --set livenessProbeHooks.hooks.exec.command[0]='/bin/bash' --set livenessProbeHooks.hooks.exec.command[1]='-c' --set livenessProbeHooks.hooks.exec.command[2]='nc -z -v -n 127.0.0.1 80' --set livenessProbeHooks.hooks.initialDelaySeconds=5 --set livenessProbeHooks.hooks.periodSeconds=10"}
            },
            "email": {
                "successful": ["shilei@zhangyue.com"],
                "failure": [""],
                "warning": [""]
            }
        }
    },
    "docker": {
        "registry": "harbor.aws.ops.zhangyue-ops.com",
        "organization": "devops",
        "project": "algae"
    }
}
